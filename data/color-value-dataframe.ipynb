    "import pandas as pd\n",
    "def color_df(val):\n",
    "    color = 'red' if val < 0 else 'green'\n",
    "    return 'color: %s' % color\n",
    "\n",
    "df = pd.DataFrame(dict(col_1=[1.53,-2.5,3.53],col_2 =[-4.1,5.9,0]))\n",
    "df.style.applymap(color_df)"
